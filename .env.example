# Copy this file to .env and uncomment/modify the values you need

# Dotfiles Configuration
# Your personal dotfiles repository for Neovim, tmux, etc.
# DOTFILES_REPO=https://github.com/beckitrue/dotfiles
# DOTFILES_INSTALL_CMD=install.sh

# AWS Configuration
# For AWS CLI access
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_DEFAULT_REGION=us-east-1
# AWS_PROFILE=default

# Cloudflare Configuration
# For Cloudflare Wrangler
# CLOUDFLARE_API_TOKEN=your_api_token_here
# CLOUDFLARE_ACCOUNT_ID=your_account_id_here

# Git Configuration
# Used if git config is not already set
# GIT_USER_NAME="Your Name"
# GIT_USER_EMAIL="your-email@users.noreply.github.com"

# Claude Code Configuration
# When claude-code CLI becomes available
# CLAUDE_API_KEY=your_api_key_here
# CLAUDE_API_ENDPOINT=https://api.anthropic.com

# Container Configuration
# WORKSPACE_DIR=/workspace
# CONTAINER_USER=vscode